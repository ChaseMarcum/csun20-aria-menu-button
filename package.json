  "author": "<schne324@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-stage-0": "^7.8.3",
    "axe-core": "^3.4.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cauldron-react": "^5.0.0",
    "deque-pattern-library": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "simulant": "^0.2.2"
  },
  "scripts": {
    "build": "parcel build demo/index.html",
    "dev": "parcel demo/index.html",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "fmt": "prettier --write *.{js,md} **/*.{js,md}",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,md,json,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "index.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "settings": {
      "react": {
        "version": "16"
      }
    },
    "overrides": [
      {
        "files": "**/*.test.js",
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 60,
    "bracketSpacing": true,
    "useTabs": false,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
